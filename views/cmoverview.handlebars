                                <textarea class="form-control" rows="3" id="supplycomments"
                                    maxlength="500">{{{result.0.supply_volumecommitmentcomments}}}</textarea>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="panel-group col-md-2">
                <div class="panel panel-default">
