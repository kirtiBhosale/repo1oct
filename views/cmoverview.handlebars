                                <label for="">Volume Commitment:</label>
                                <select class="form-control" id="volumecommitment">
                                    {{#each SelVolumeCommitment}}
                                    <option {{#this.Selected}}selected="selected" {{/this.Selected}} value="{{this.Id}}"
