                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div style="display:none;" class="form-group agreementdate">
                                <label>QTA Approval Date: <span class="mandatory-star">*</span></label>
                                <div class='input-group date' id='datetimepicker2'>
                                    <input value="{{{modifydate agreementapprovaldate}}}" type='text' id="approvaldate"
                                        class="form-control" value='{{{result.0.quality_reviewdate}}}' />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div style="display:none;" class="form-group agreementdate">
                                <label>QTA Review Date: <span class="mandatory-star">*</span></label>
                                <div class='input-group date' id='datetimepicker3'>
                                    <input value="{{{modifydate agreementreveiewdate}}}" type='text' id="reviewdate"
                                        class="form-control" name="rd" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            {{else}}
                            <div class="form-group agreementdate">
                                <label>Initial QTA Signature Date: </label>
                                <div class='input-group date' id='datetimepicker3'>
                                    <input value="{{{modifydate qtasigneddate}}}" type='text' id="qtasigneddate"
                                        class="form-control" name="rd" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group agreementdate">
                                <label>QTA Approval Date: <span class="mandatory-star">*</span></label>
                                <div class='input-group date' id='datetimepicker2'>
                                    <input value="{{{modifydate agreementapprovaldate}}}" type='text' id="approvaldate"
                                        class="form-control" value='{{{result.0.quality_reviewdate}}}' />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group agreementdate">
                                <label>QTA Review Date: <span class="mandatory-star">*</span></label>
                                <div class='input-group date' id='datetimepicker3'>
                                    <input value="{{{modifydate agreementreveiewdate}}}" type='text' id="reviewdate"
                                        class="form-control" name="rd" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            {{/if}}
